{
    "name": "squatto/grafite-formmaker",
    "description": "A remarkably magical form and input maker tool for Laravel.",
    "license": "MIT",
    "authors": [
        {
            "name": "Scott Carpenter",
            "email": "scott@payforstay.com",
            "role": "Developer"
        },
        {
            "name": "Matt Lantz",
            "email": "mattlantz@gmail.com",
            "role": "Original package developer"
        }
    ],
    "require": {
        "php": "^7.2",
        "doctrine/dbal": "^2.9",
        "illuminate/support": "^6.0",
        "laravelcollective/html": "^6.0"
    },
    "require-dev": {
        "roave/security-advisories": "dev-master",
        "phpunit/phpunit": "^8.0",
        "mockery/mockery": "^1.0",
        "mikey179/vfsstream": "^1.6",
        "orchestra/database": "^4.0",
        "orchestra/testbench": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "Grafite\\FormMaker\\": "src/"
        },
        "files": [
          "src/Helpers/form_maker.php",
          "src/Helpers/input_maker.php"
        ]
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Grafite\\FormMaker\\FormMakerProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
